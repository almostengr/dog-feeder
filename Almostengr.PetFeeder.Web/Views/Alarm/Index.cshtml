@model IList<AlarmDto>


<h1>Recent Alarms</h1>

@if (Model.Count > 0)
{
    <div class="row">
        <div class="col bg-dark text-white">Date/Time</div>
        <div class="col bg-dark text-white">Scheduled</div>
        <div class="col bg-dark text-white">Amount</div>
    </div>

    @foreach (var item in Model)
    {
        <div class="row">
            <div class="col px-1">@item.Created</div>
            <div class="col px-1">@item.WasScheduled</div>
            <div class="col px-1">@item.Amount</div>
        </div>
    }
}
else {
    <h2 class="text-center">No feedings have occurred.</h2>
}